{
  "manifest_version": 3,
  "name": "DiplomacyTranslater",
  "description": "ヨーロッパの土地の名前なんか分からん",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
